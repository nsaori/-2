//2013110023 노다 사오리

import java.awt.*;
import javax.swing.*;

public class MyFrame extends JFrame {

	public MyFrame(){
		this.setTitle("2013110023 노다 사오리");
		this.setSize(300, 250);
		
		MyPanel p = new MyPanel();
		this.add(p);
		
		this.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		this.setVisible(true);
		
	}
}
