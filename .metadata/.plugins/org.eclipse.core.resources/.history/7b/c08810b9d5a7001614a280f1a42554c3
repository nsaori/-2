import java.awt.Button;
import java.awt.*;

import javax.swing.JPanel;

public class MyPanal extends JPanel {
	public MyPanal() {
		
		JPanel p = new JPanel();
		p.setLayout(new GridLayout(3, 2, 30, 25));
		
		p.add(new Button("1"));
		p.add(new Button("2"));
		p.add(new Button("3"));
		p.add(new Button("4"));
		p.add(new Button("5"));
		p.add(new Button("6"));
		
		this.add(p);
	}
}
